{
  "name": "marfrigwebapp_updated",
  "version": "1.0.0",
  "description": "Marfrig Web App",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t marfrigwebapp_updated .",
    "docker:run": "docker run -p 3000:3000 -d marfrigwebapp_updated",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "start-prod": "pm2 start ecosystem.config.js --env production",
    "stop-prod": "pm2 stop ecosystem.config.js --env production"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.0.2",
    "@loopback/boot": "^3.0.2",
    "@loopback/booter-lb3app": "^2.2.12",
    "@loopback/context": "^3.12.0",
    "@loopback/core": "^2.11.0",
    "@loopback/http-server": "^2.3.0",
    "@loopback/openapi-v3": "^5.0.0",
    "@loopback/repository": "^3.1.0",
    "@loopback/rest": "^8.0.0",
    "@loopback/rest-crud": "^0.8.16",
    "@loopback/rest-explorer": "^3.0.2",
    "@loopback/service-proxy": "^3.0.2",
    "@sap/hana-client": "^2.5.104",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "helmet": "^3.23.3",
    "loopback": "^3.26.0",
    "loopback-boot": "^3.3.1",
    "loopback-component-explorer": "^6.5.0",
    "loopback-component-logger": "^1.0.0",
    "loopback-component-model-diagram": "^0.4.2",
    "loopback-component-roles": "^1.0.1",
    "loopback-component-visualizer": "^1.2.1",
    "loopback-connector-postgresql": "^5.0.2",
    "loopback-connector-saphana": "^0.7.2",
    "pm2": "^4.5.4",
    "serve-favicon": "^2.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.5",
    "@loopback/eslint-config": "^10.0.1",
    "@loopback/testlab": "^3.2.7",
    "@types/node": "^10.17.37",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "cross-env": "^7.0.0",
    "eslint": "^7.10.0",
    "eslint-config-loopback": "^13.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-mocha": "^8.0.0",
    "nsp": "^3.2.1",
    "source-map-support": "^0.5.19",
    "typescript": "~4.0.3"
  }
}
